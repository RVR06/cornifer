{
	"comments": {
		"lineComment": "#"
	},
	"brackets": [
		["{", "}"]
	],
	"autoClosingPairs": [
		{ "open": "{", "close": "}" },
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
	],
	"autoCloseBefore": ";:.,=}])>` \n\t",
	"surroundingPairs": [
		["{", "}"],
		["\"", "\""]
	],
	"folding": {
		"markers": {
		  "start": "^\\s*#region",
		  "end": "^\\s*#endregion"
		}
	}
}